generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("postgres://postgres:'Saad@645'@localhost:5432/akhtardb")
}

model User {
  id        Int      @id @default(autoincrement())
  username  String   @unique
  password  String   @unique
  name      String   @unique
  department String
  designation String
  cnic      String   @unique
  code      String   @unique
  manager   String
  bank      String   
  phone     String   
  access    String
  account   String
  // createdBy String   
  // updatedBy String     
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

}
